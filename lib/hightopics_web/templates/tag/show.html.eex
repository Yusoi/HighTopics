<h2>Show Tag</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @tag.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @tag.description %>
  </li>

</ul>

<span><%= link "Edit", to: tag_path(@conn, :edit, @tag) %></span>
<span><%= link "Back", to: tag_path(@conn, :index) %></span>
